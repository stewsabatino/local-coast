{{#each user.items as |item|}}
        <div id="comments-wrapper">
            <a id="add-to-home" href="/newplaylist/{{item.id}}" value="{{item.id}}">
                <img src="{{item.images.[0].url}}"/>
                <h2>{{item.name}}</h2>
                
            </a>
        </div>
{{/each}}